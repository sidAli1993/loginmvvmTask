<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@+id/navSplash">

    <fragment
        android:id="@+id/navSplash"
        android:name="com.sid_ali_tech.loginapptaskmvvm.presentation.ui.auth.splash.SplashFragment"
        android:label="Splash"
        tools:layout="@layout/fragment_splash" />

    <fragment
        android:id="@+id/navLogin"
        android:name="com.sid_ali_tech.loginapptaskmvvm.presentation.ui.auth.login.LoginFragment"
        android:label="Login"
        tools:layout="@layout/fragment_login" />
    <fragment
        android:id="@+id/navRegister"
        android:name="com.sid_ali_tech.loginapptaskmvvm.presentation.ui.auth.signup.SignUpFragment"
        android:label="Login"
        tools:layout="@layout/fragment_sign_up" />
    <fragment
        android:id="@+id/navForgetPwd"
        android:name="com.sid_ali_tech.loginapptaskmvvm.presentation.ui.auth.forgetpassword.ForgetPasswordFragment"
        android:label="Login"
        tools:layout="@layout/fragment_forget_password" />
    <fragment
        android:id="@+id/navDashboard"
        android:name="com.sid_ali_tech.loginapptaskmvvm.presentation.ui.dashboard.DashboardFragment"
        android:label="Login"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navEditUser"
        android:name="com.sid_ali_tech.loginapptaskmvvm.presentation.ui.auth.editscreen.EditUser"
        android:label="Edit User"
        tools:layout="@layout/fragment_edit_user" />
</navigation>